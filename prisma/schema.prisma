generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model submission {
  id     Int    @id @default(autoincrement())
  aadhar String
  name   String
}

model otp {
  id  Int @id @default(autoincrement())
  otp Int
}
